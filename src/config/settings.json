{
  "api": {
    "port": "6000",
    "prefix": "api",
    "cache": {
      "max": 1000,
      "ttl": 60
    },
    "throttler": {
      "limit": 1000,
      "ttl": 60
    },
    "rabbitmq":{
      "name": "PAYMENT_SERVICE",
      "options": {
        "urls": ["amqp://guest:guest@localhost:5672"],
        "queue": "payment-service-queue",
        "queueOptions": {
          "durable": true
        }
      }
    }
  }
}